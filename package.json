    "@storybook/addon-links": "^6.2.0-alpha.5",
    "@storybook/vue": "^6.2.0-alpha.5",
    "axios": "^0.19",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0",
    "laravel-mix": "^5.0.1",
    "lodash": "^4.17.19",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.15.2",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12"
  },
